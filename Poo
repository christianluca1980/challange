<html>

<script>

class Contrasena {

    constructor(longitud, contrasena){
        this.longitud = longitud ? longitud : 8;
        this._contrasena = contrasena ? contrasena : Contrasena.generatePassword(this.longitud);
    }

    get longitud() {
        return this._longitud;
    }
    set longitud(newLongitud) {
        this._longitud = newLongitud;
    }

    get contrasena() {
        return this._contrasena;
    }
    /*set contrasena(newContrasena) {
        this._contrasena = newContrasena;
    }*/

    static generatePassword(longitud) {
    var longitud = longitud,
        caracteres = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!#><?=)(",
        valor = "";
    for (var i = 0, n = caracteres.length; i < longitud; ++i) {
        valor += caracteres.charAt(Math.floor(Math.random() * n));
    }
    return valor;
}
}

let contrasena1 = new Contrasena();
console.log(contrasena1);

</script>


<body>

</body>

</html>
